{
  "name": "ywasm-benchmarks",
  "version": "1.0.0",
  "description": "Ywasm benchmarks",
  "main": "./run.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "measure-bundle": "([ -f \"./dist/bundle.js\" ] || npm run dist) && node ../../bin/measure-bundle.js node_modules/ywasm/ywasm_bg.wasm",
    "start": "npm run measure-bundle && node --expose-gc run.js && npm run table",
    "start:bun": "npm run measure-bundle && bun run run.js && npm run table",
    "start:browser": "rollup -c && 0serve -o benchmark.html",
    "table": "echo 'Ywasm results: \n\n' && node ../../bin/render-table.js ../results.json 6000 ywasm",
    "dist": "npm run clean && rollup -c && gzip --keep dist/*"
  },
  "author": "Kevin Jahns",
  "license": "MIT",
  "dependencies": {
    "ywasm": "^0.17.4"
  }
}
